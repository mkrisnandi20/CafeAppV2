<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sqliteandroidstudiojava">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Login"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".RegisterAct"
            android:exported="false"
            android:label="Register"/>
        <activity
            android:name=".LoginAct"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/menu_cafe"
            android:theme="@style/MaintTheme">

        </activity>
        <activity
            android:name=".DetailActivity"
            android:label="@string/detail_menu"
            android:theme="@style/DetailTheme" />
        <activity
            android:name=".AddActivity"
            android:theme="@style/AddTheme" />
    </application>

</manifest>